'''
#program9:

#two strings from user

#mathematics ===> 4 vowels
#science ==> 3 vowels

#both are equal count or not equal
'''
a = input("Give your string  : ")
a = a.lower()

a1 = a.count("a")
#print("mathematics :",a1)
a2 = a.count("e")
#print("mathematics :",a2)
a3 = a.count("i")
#print("mathematics :",a3)
a4 = a.count("o")
#print("mathematics :",a4)
a5 = a.count("u")
#print("mathematics :",a5)
cnt = a1 + a2 + a3 + a4 + a5
print(cnt)

b = input("Give your string b : ")
b = b.lower()

b1 = b.count("a")

b2 = b.count("e")

b3 = b.count("i")

b4 = b.count("o")

b5 = b.count("u")

cnt2 = b1 + b2 + b3 + b4 + b5
print(cnt2)

if cnt == cnt2:
    print("both vowel are equal count")
else:
    print("a and b vowel count not equal")

