'''
#program6
#hackerrank Write a function

def is_leap(n):
    if n % 400 == 0:
        return True
    if n % 100 == 0:
        return False
    if n % 4 == 0:
        return True
    return False

print(is_leap(int(input())))
'''

def is_leap(year):
    leap = False
    
    # Write your logic here
    if year % 400 == 0:
        return True
    if year % 100 == 0:
        return False
    if year % 4 == 0:
        return True
    return False

year = int(input("Give your input year : "))
print(is_leap(year))

if is_leap(year):
    #print("The ASCII value of '" + c + "' is", ord(ascii_str[len(ascii_str)//2]))
    print("You entered the year '" + str(year) + "'is a leap year" )
else:
    print("You entered the year '" + str(year) + "'is not a leap year")

